#include <bits/stdc++.h>

using namespace std;

int main()
{
    map<string,int> a;
    a["Naveen"]=75;
    a["Sahil"]=55;
    a["Singroha"]=88;
     a.insert({{"VED",1000},{"SARPANCH",100}});
    map<string,int> :: iterator it;
    for(it=a.begin();it!=a.end();it++){
        cout<<(*it).first<<" "<<(*it).second<<endl;
    }
    cout<<a.size()<<"\n"<<a.max_size()<<"\n"<<a.empty();
   
    return 0;
} 
